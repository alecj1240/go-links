{
  "manifest_version": 3,
  "name": "My Go Links",
  "version": "1.0",
  "description": "Custom go links for Chrome.",
  "permissions": ["webNavigation", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  }
}